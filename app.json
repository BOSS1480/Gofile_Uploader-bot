{
  "name": "Gofile Uploader Bot",
  "description": "A Telegram bot",
  "repository": "https://github.com/BOSS1480/Gofile_Uploader-bot/tree/main",
  "keywords": ["telegram", "bot", "pyrogram", "python"],
  "stack": "heroku-22",
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID from my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API Hash from my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Your Telegram Bot Token from @BotFather",
      "required": true
    },
    "MUST_JOIN": {
      "description": "Username/ID of your Telegram channel/group (optional)",
      "required": false
    }
  }
}
